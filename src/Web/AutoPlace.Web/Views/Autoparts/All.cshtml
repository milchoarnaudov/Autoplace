@model IEnumerable<AutopartsListItemViewModel>

@{ 
    this.ViewData["Title"] = "All Autoparts";
}
<h1>All Autoparts</h1>

<div>
    @if (Model.Count() == 0)
    {
        <div>Empty</div>
    }
    else
    {
        <div class="row">
            @foreach (var autopart in Model)
            {
               <partial name="_AutopartPartial" model="autopart" />
            }
        </div>
    }
</div>
