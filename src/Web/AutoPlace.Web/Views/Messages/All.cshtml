@model IEnumerable<MessageListItemViewModel>

@{
    this.ViewData["Title"] = "All Messages";
}

<h1>All Messages</h1>

<div>
    @if (Model.Count() == 0)
    {
        <div>Empty</div>
    }
    else
    {
        <div class="row">
            @foreach (var message in Model)
            {
                <partial name="_MessagePartial" model="message" />
            }
        </div>
    }
</div>

